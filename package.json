{
  "name": "gastropod-task-sass",
  "version": "0.0.1",
  "description": "gastropod addon",
  "main": "index.js",
  "keywords": [
    "gastropod",
    "gastropod-task",
    "build",
    "static site"
  ],
  "config": {
    "type": "task"
  },
  "scripts": {
    "merge-to-master": "git checkout master && git merge develop",
    "merge-to-develop": "git checkout develop && git merge master",
    "push": "git push --all && git push --tags",
    "preversion": "npm run merge-to-master",
    "postversion": "npm run merge-to-develop && npm run push"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "check-args": "0.0.6",
    "debug": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "vinyl-named": "^1.1.0"
  }
}
